<html>
    <head>
        <title>Register Form</title>
        <link rel="stylesheet" type="text/css" href="style.css"/>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
    </head>
    <body>
        <div class="registerform">
            <h5>Sign up now!</h5>
            <form>
                <input id='firstNameEntry' type='text' placeholder='First Name' value=''/><br>
                <input id='lastNameEntry' type='text' placeholder='Last Name' value=''/><br>
                <input id='emailEntry' type='email' placeholder='Email' value=''/><br>
                <input id='addressEntry' type='text' placeholder='Address' value=''/><br>
                <input id='zipEntry' type='text' placeholder='Zip Code' value=''/><br>
                <input id='stateEntry' type='text' placeholder='State' value=''/><br>
                <input id='idBar' type='text' placeholder='User Name' value=''/><br>
                <input id='pwBar1' type='password' placeholder='Password' value=''/><br>
                <input id='pwBar2' type='password' placeholder='Confirm Password' value=''/><br>
                <input id='submitBtn' type='button' value='Register'/><br>
            </form>
                <p><a href="/login">Sign-in</a> if you already have an account.</p>
        </div>
        <script src="newLogin.js"></script>
    </body>
</html>
