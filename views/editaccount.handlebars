<html>
    <head>
        <title>Change Details</title>
        <link rel="stylesheet" type="text/css" href="style.css"/>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
    </head>
    <body>
        <div class="detailchange">
            <h5>Change Account Details!</h5>
            <div id="invalidDiv"></div>
            <form>
            
                <input id='firstNameEntry' type='text' placeholder='First Name' value='{{firstName}}' disabled/><br>
                <input id='lastNameEntry' type='text' placeholder='Last Name' value='{{lastName}}' disabled/><br>
                <input id='emailEntry' type='email' placeholder='Email' value='{{email}}' disabled/><br>
                <input id='addressEntry' type='text' placeholder='Address' value='{{address}}'/><br>
                <input id='zipEntry' type='text' placeholder='Zip Code' value='{{zipcode}}'/><br>
                <input id='stateEntry' type='text' placeholder='State' value='{{state}}'/><br>
                <input id='idBar' type='text' placeholder='User Name' value='{{userName}}'/><br>
                <input id='pwBar1' type='password' placeholder='Password' value='{{password}}'/><br>
                <input id='pwBar2' type='password' placeholder='Confirm Password' value='{{password}}'/><br>
                <input id='submitBtn' type='button' value='Register'/><br>
                               
                <input id='btn' type='button' value='Submit Changes'/><br>
        </div>
    </body>
</html>
